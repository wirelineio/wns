{
  "name": "@wirelineio/wns-docs",
  "version": "1.0.0-beta.9",
  "description": "DXOS Documentation",
  "keywords": [
    "dxos",
    "gatsby",
    "docs"
  ],
  "license": "GPL-3.0",
  "author": "info@dxos.org",
  "private": true,
  "scripts": {
    "clean": "rimraf .cache && rimraf public",
    "dev": "gatsby develop --port 8003",
